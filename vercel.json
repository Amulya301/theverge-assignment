{
    "version": 2,
    "builds": [
      {
        "use": "@vercel/python",
        "entrypoint": "python build.py",
        "config": {
          "maxLambdaSize": "50mb",
          "python": "3.8.1"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "build.py" }
    ],
    "env": {
      "API_KEY": "myapikey"
    }
  }
  